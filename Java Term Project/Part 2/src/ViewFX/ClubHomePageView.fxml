<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="441.0" prefWidth="724.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ClubHomePageController">
   <children>
      <HBox alignment="CENTER" layoutX="34.0" layoutY="115.0" prefHeight="261.0" prefWidth="594.0">
         <children>
            <VBox>
               <children>
                  <JFXButton alignment="CENTER" buttonType="RAISED" onAction="#showMyPlayers" prefHeight="88.0" prefWidth="326.0" text="My Players" textFill="#a84a4a">
                     <font>
                        <Font name="Bookman Old Style Bold" size="36.0" />
                     </font>
                  </JFXButton>
                  <JFXButton alignment="CENTER" buttonType="RAISED" onAction="#showBuyablePlayers" prefHeight="88.0" prefWidth="326.0" text="Buy Players" textFill="#a84a4a">
                     <font>
                        <Font name="Bookman Old Style Bold" size="36.0" />
                     </font>
                  </JFXButton>
                  <JFXButton alignment="CENTER" buttonType="RAISED" onAction="#SearchPlayers" prefHeight="88.0" prefWidth="325.0" text="Search Players" textFill="#a84a4a">
                     <font>
                        <Font name="Bookman Old Style Bold" size="36.0" />
                     </font>
                  </JFXButton>
               </children>
            </VBox>
            <ImageView fx:id="logo" fitHeight="200.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
      <Label fx:id="welcomeMessage" alignment="CENTER" layoutX="70.0" layoutY="53.0" prefHeight="40.0" prefWidth="585.0" text="Welcome Message">
         <font>
            <Font name="Calisto MT Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
